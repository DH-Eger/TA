  <msItem>
   <!--kérdéses
   <author>
   <respStmt>
   -->
   <title type="uniform" >Tihanyi alapítólevél</title>
   <!--kérdéses
   <rubric>
   <incipit>
   <explicit>
   -->
   <textLang mainLang="la" otherLangs="hu">Latin magyar szórványrészletekkel</textLang>
  </msItem>
  <!--Lehet még <msItemStruct> és <summary>, de nekünk ehhez nem kellenek-->
 </msContents>
 <physDesc><!-- kötött a tagek sorrendje!-->
 <!--Ha van szöveges leírás, akkor annak meg kell előznie a tageket-->
  <objectDesc form="establishing charter"><!-- két része van: supportDesc és layoutDesc-->
   <supportDesc material ="perg">
    <support>
     <p><!--Ide jön az oklevél fizikai tulajdonságainak leírása.--></p>
    </support>
    <extent><!--Kérdéses, hogy ez kell-e a méretleíráshoz. Kódexekhez használatos inkább az <extent> alapvetően a terjedelmet írja le, viszont benne lehet a <dimensions> v. <measure>, de lehet sima <p> is.-->
	<dimensions>
      <height></height>
      <width></width>
     </dimensions>
    </extent>
	<!--Kódexeknél még:
	<collation></collation>
	<foliation></foliation>-->
	<condition><!--Állapotleírás Ruditól?--></condition>
   </supportDesc>
   <layoutDesc>
    <layout><!--Vannak használható attribútumok, de lehet sima <p> is.-->
     <p></p>
    </layout>
   </layoutDesc>
  </objectDesc>
